{
  "name": "object-set-type",
  "version": "3.0.0",
  "description": "A Set implementation which supports Objects.",
  "main": "index.js",
  "types": "index.d.ts",
  "module": "index.mjs",
  "scripts": {
    "build": "npm run buildUpdate && npm test && npm run typedoc",
    "buildUpdate": "tsup && npm run copyToDist",
    "copyToDist": "copyfiles -f *.md package.json dist",
    "typedoc": "typedoc",
    "clean": "rimraf ./dist ./docs ./reports ./coverage",
    "test": "dotenv-ng --var JEST_JUNIT_OUTPUT_DIR=./reports --- node --experimental-vm-modules ./node_modules/.bin/jest --coverage",
    "lint": "eslint . --ext .ts --fix",
    "compile": "tsc && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/szikszail/object-set-type.git"
  },
  "keywords": [
    "Set",
    "Object"
  ],
  "files": [
    "**/*.js",
    "**/*.mjs",
    "**/*.d.ts",
    "**/*.d.mts",
    "**/*.js.map",
    "**/*.mjs.map"
  ],
  "engines": {
    "node": ">=12.0.0"
  },
  "author": "Laszlo Szikszai <sziklaszlo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/szikszail/object-set-type/issues"
  },
  "homepage": "https://github.com/szikszail/object-set-type#readme",
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^22.5.4",
    "@typescript-eslint/eslint-plugin": "^8.5.0",
    "@typescript-eslint/parser": "^8.5.0",
    "copyfiles": "^2.4.1",
    "dotenv-ng": "^1.4.0",
    "eslint": "^9.10.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "typedoc": "^0.26.7",
    "typescript": "^5.6.2",
    "tsup": "^8.2.4"
  },
  "dependencies": {
    "deep-eql": "^5.0.2"
  },
  "tsup": {
    "entry": [
      "src/index.ts"
    ],
    "dts": true,
    "splitting": true,
    "sourcemap": true,
    "clean": true,
    "minify": true,
    "format": [
      "esm",
      "cjs"
    ]
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transform": {
      "^.+\\.ts?$": [
        "ts-jest",
        {
          "useESM": true
        }
      ]
    },
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "dist"
    ],
    "coverageDirectory": "./coverage",
    "collectCoverageFrom": [
      "./src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  }
}
